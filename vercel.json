{
  "rewrites": [
    {
      "source": "/:category/:postId",
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": "(facebookexternalhit|Twitterbot|LinkedInBot|WhatsApp)"
        }
      ],
      "destination": "/meta/:category-:postId.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 
